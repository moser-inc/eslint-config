{
  "name": "@moser-inc/eslint-config",
  "version": "2.3.4",
  "type": "module",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": "./dist/index.mjs",
    "./flat": "./dist/index.mjs"
  },
  "typesVersions": {
    "*": {
      "flat": [
        "./dist/index.d.mts"
      ],
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "prepack": "npm run build",
    "lint": "eslint -c eslint.config.ts && npm run lint:typeaware",
    "lint:typeaware": "eslint -c eslint.config.typeaware.ts",
    "publint": "npx -y publint@latest && npx -y @arethetypeswrong/cli@latest --pack --profile esm-only"
  },
  "peerDependencies": {
    "eslint": ">= 6",
    "prettier": ">= 1"
  },
  "dependencies": {
    "@eslint/js": "^9.39.2",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-flat-config-utils": "^2.1.4",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-depend": "^1.4.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-unicorn": "^62.0.0",
    "typescript-eslint": "^8.53.0"
  },
  "devDependencies": {
    "eslint-typegen": "^2.3.0",
    "unbuild": "^3.6.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moser-inc/eslint-config.git",
    "directory": "packages/core"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/moser-inc/eslint-config/issues"
  },
  "homepage": "https://github.com/moser-inc/eslint-config#readme"
}
