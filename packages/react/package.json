{
  "name": "@moser-inc/eslint-config-react",
  "version": "5.1.0",
  "type": "module",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": "./dist/index.mjs",
    "./flat": "./dist/index.mjs"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "prepack": "npm run build",
    "lint": "eslint -c eslint.config.ts && npm run lint:typeaware",
    "lint:typeaware": "eslint -c eslint.config.typeaware.ts",
    "publint": "npx -y publint@latest && npx -y @arethetypeswrong/cli@latest --pack --profile esm-only"
  },
  "peerDependencies": {
    "eslint": ">= 6",
    "prettier": ">= 1"
  },
  "dependencies": {
    "@eslint-react/eslint-plugin": "^2.12.2",
    "@moser-inc/eslint-config": "3.0.0",
    "eslint-plugin-react-hooks": "^7.0.1"
  },
  "devDependencies": {
    "eslint-typegen": "^2.3.0",
    "unbuild": "^3.6.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moser-inc/eslint-config.git",
    "directory": "packages/react"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/moser-inc/eslint-config/issues"
  },
  "homepage": "https://github.com/moser-inc/eslint-config#readme"
}
